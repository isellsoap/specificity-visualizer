{
  "name": "specificity-visualizer",
  "repository": "isellsoap/specificity-visualizer",
  "author": "Francesco Schwarz",
  "description": "A visual way to analyze the specificity of selectors in CSS.",
  "homepage": "https://isellsoap.github.io/specificity-visualizer/",
  "bugs": {
    "url": "https://github.com/isellsoap/specificity-visualizer/issues"
  },
  "license": "MIT",
  "engines": {
    "node": "8.11.4",
    "npm": "5.6.0"
  },
  "scripts": {
    "build": "webpack --config=webpack.config.ts --env.production=true -p --progress --colors",
    "normalize-to-sass": "cp node_modules/normalize.css/normalize.css src/styles/vendor/_normalize.scss",
    "postinstall": "npm run normalize-to-sass",
    "start": "npm install && npm run build",
    "watch": "webpack --config=webpack.config.ts --env.production=false -d --watch --progress --colors"
  },
  "dependencies": {
    "highcharts": "6.1.1",
    "javascript-natural-sort": "0.7.1",
    "lodash": "4.17.10",
    "normalize.css": "8.0.0",
    "specificity": "0.4.0",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@types/webpack": "3.8.1",
    "autoprefixer": "9.1.3",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "0.28.7",
    "eslint": "5.4.0",
    "eslint-loader": "2.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.9.3",
    "postcss": "7.0.2",
    "postcss-loader": "3.0.0",
    "postcss-sass": "0.3.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.22.1",
    "stylelint": "8.3.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "0.8.0",
    "stylelint-webpack-plugin": "0.9.0",
    "ts-node": "4.0.1",
    "typescript": "2.6.2",
    "webpack": "3.10.0"
  }
}
